<?xml version='1.0' encoding='utf-8'?>
<widget id="accurascan.hybrid.com" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova_Accurascan_Kyc</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />

    <platform name="android">
        <preference name="android-targetSdkVersion"  value="32" />
        <!-- <preference name="android-minSdkVersion" value="32" />
        <preference name="android-compileSdkVersion" value="32" /> -->
        <preference name="android-minSdkVersion" value="21" />
    </platform>
    <platform name="ios">
        <preference name="deployment-target" value="12.0" />
        <!-- Add other platform-specific preferences here -->
    </platform>
</widget>
